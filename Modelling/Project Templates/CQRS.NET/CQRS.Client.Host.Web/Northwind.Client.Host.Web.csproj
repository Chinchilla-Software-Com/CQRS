  <PropertyGroup>
    <PreBuildEvent>echo Param( &gt; $(ProjectDir)prepare.ps1
echo 	[string] $sourceFolder, &gt;&gt; $(ProjectDir)prepare.ps1
echo 	[string] $targetFolder, &gt;&gt; $(ProjectDir)prepare.ps1
echo 	[string] $filter ## *.css,*.js,*.png,*.jpg,*.gif,*.htm,*.html &gt;&gt; $(ProjectDir)prepare.ps1
echo ^) &gt;&gt; $(ProjectDir)prepare.ps1

echo $source = "$sourceFolder\*" &gt;&gt; $(ProjectDir)prepare.ps1

echo Get-ChildItem -Path "$sourceFolder\*" -Recurse -Filter $filter ^|  foreach { &gt;&gt; $(ProjectDir)prepare.ps1
echo 	$DestFile =  $_.Fullname.Substring($sourceFolder.Length - 1) &gt;&gt; $(ProjectDir)prepare.ps1
echo 	$DestFile =  "$targetFolder\$DestFile" &gt;&gt; $(ProjectDir)prepare.ps1
echo 	$null = New-Item -Path  $DestFile -Type File -Force &gt;&gt; $(ProjectDir)prepare.ps1
echo 	Copy-Item -Path  $_.FullName -Destination $DestFile -Force &gt;&gt; $(ProjectDir)prepare.ps1
echo } &gt;&gt; $(ProjectDir)prepare.ps1

echo Param( &gt; $(ProjectDir)download.ps1
echo 	[string] $url, &gt;&gt; $(ProjectDir)download.ps1
echo 	[string] $path &gt;&gt; $(ProjectDir)\download.ps1
echo ^) &gt;&gt; $(ProjectDir)download.ps1

echo [System.Net.ServicePointManager]::ServerCertificateValidationCallback = {$true} &gt;&gt; $(ProjectDir)download.ps1
echo $WebClient = New-Object net.webclient &gt;&gt; $(ProjectDir)download.ps1
echo $path="$path\index.htm" &gt;&gt; $(ProjectDir)download.ps1
echo ## $WebClient.DownloadFile($url, $path) &gt;&gt; $(ProjectDir)download.ps1

echo $content=$WebClient.DownloadString($url) &gt;&gt; $(ProjectDir)download.ps1
echo $fileContent = $content.Replace("/$ext_safeprojectname$/Client", "") &gt;&gt; $(ProjectDir)download.ps1
echo $fileContent ^| Out-File $path &gt;&gt; $(ProjectDir)download.ps1</PreBuildEvent>
